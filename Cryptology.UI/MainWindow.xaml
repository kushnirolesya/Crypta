<Window x:Class="Cryptology.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cryptology.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="Frame" NavigationUIVisibility="{Binding NavigationUIVisibility}" Content="{Binding CurrentPage, Converter={local:ApplicationPageValueConverter}}">
            
        </Frame>
    </Grid>
</Window>
